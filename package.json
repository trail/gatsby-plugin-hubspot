  },
  "devDependencies": {
    "@austinleegordon/prettier-config": "^1.0.0",
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.5",
    "babel-preset-gatsby-package": "^2.16.0",
    "cross-env": "^7.0.3",
    "jest": "^28.1.1",
